{
  "rewrites": [
    {
      "source": "/:slug",            
      "has": [
        {
          "type": "host",
          "value": "shrtit.tech"
        }
      ],
      "destination": "https://api.shrtit.tech/:slug"
    },
    {
      "source": "/(.*)",               
      "destination": "/index.html"
    }
  ]
}
