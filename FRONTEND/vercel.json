{
  "rewrites": [
    {
      "source": "/:slug",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country" 
        }
      ],
      "destination": "https://api.shrtit.tech/:slug"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://api.shrtit.tech/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
